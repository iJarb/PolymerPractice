{
  "entrypoint": "index.html",
  "shell": "src/my-profile-app.html",
  "fragments": [
    "src/my-profile-navigator.html",
    "src/my-profile-home.html",
    "src/my-profile-login.html",
    "src/my-profile-register.html"
  ],
  "sources": [
    "src/**/*",
    "images/**/*",
    "bower.json"
  ],
  "extraDependencies": [
    "manifest.json",
    "bower_components/webcomponentsjs/*"
  ],
  "lint": {
    "rules": [
      "polymer-2"
    ]
  },
  "builds": [
    { "preset": "es5-bundled" },
    { "preset": "es6-unbundled" }
  ]
}